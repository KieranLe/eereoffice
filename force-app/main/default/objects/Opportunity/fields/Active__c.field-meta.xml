<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Active__c</fullName>
    <description>Checks if the contract is Active or Not.</description>
    <externalId>false</externalId>
    <formula>Contract_Start_Date__c &lt;= TODAY() &amp;&amp; TODAY() &lt;= Contract_End_Date__c</formula>
    <label>Active</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
